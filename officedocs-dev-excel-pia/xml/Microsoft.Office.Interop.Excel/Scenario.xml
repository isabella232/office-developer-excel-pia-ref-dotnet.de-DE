    <Member MemberName="ChangeScenario">
      <MemberSignature Language="C#" Value="public object ChangeScenario (object ChangingCells, object Values);" />
      <MemberSignature Language="ILAsm" Value=".method public hidebysig newslot virtual instance object ChangeScenario([in]object ChangingCells, [in]object Values) runtime managed" />
